{
  "name": "reaware",
  "version": "0.1.0",
  "main": "public/index.html",
  "author": "David Bailey <davidbailey00@outlook.com>",
  "window": {
    "title": "ReAware",
    "width": 640,
    "height": 480,
    "min_width": 640,
    "min_height": 480
  },
  "scripts": {
    "install-sdk": "NWJS_BUILD_TYPE=sdk pnpm install",
